<% const postSlugs = collections.posts.map(post => post.fileSlug); %>
<% const pageSlugs = collections.pages.map(page => page.fileSlug); %>
<%- postSlugs.map(postSlug => `/posts/${postSlug}`).concat(pageSlugs.map(pageSlug => `/pages/${pageSlug}`)).join("\n"); -%>