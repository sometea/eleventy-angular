{
  "name": "eleventy-angular",
  "version": "1.0.0",
  "scripts": {
    "eleventy-start": "eleventy --serve",
    "eleventy-build": "eleventy",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run eleventy-angular:serve-ssr",
    "serve:ssr": "node dist/eleventy-angular/server/main.js",
    "build:ssr": "ng build --prod && ng run eleventy-angular:server:production",
    "prerender": "ng run eleventy-angular:prerender --routesFile _site/api/routes.txt",
    "prerender:ci": "npm run eleventy-build && start-server-and-test dev:ssr http://localhost:4200 prerender",
    "deploy": "firebase deploy",
    "all:ci": "npm run prerender:ci && npm run deploy"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.2.1",
    "@angular/common": "~10.2.1",
    "@angular/compiler": "~10.2.1",
    "@angular/core": "~10.2.1",
    "@angular/forms": "~10.2.1",
    "@angular/platform-browser": "~10.2.1",
    "@angular/platform-browser-dynamic": "~10.2.1",
    "@angular/platform-server": "~10.2.1",
    "@angular/router": "~10.2.1",
    "@nguniversal/express-engine": "^10.1.0",
    "express": "^4.15.2",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@angular-devkit/build-angular": "^0.1002.0",
    "@angular/cli": "~10.2.0",
    "@angular/compiler-cli": "~10.2.1",
    "@nguniversal/builders": "^10.1.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "angular-prerender": "^6.0.5",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "start-server-and-test": "^1.11.5",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.5"
  }
}
